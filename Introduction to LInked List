#include <iostream>
using namespace std;
class node
{
    
public:
    int data;
    node* next;
    node(int x)
    {
        data=x;
        next=NULL;
    }
    
};
void pushback(node* &head,int data)
{
    node* n=new node(data);
    if(head==NULL)
    {
        head=n;
        return;
    }
    node* current=head;
    
    while(current->next != NULL)
    {
        current=current->next;
    }
    current->next=n;
}


void display(node* &head)
{
    while(head!=NULL)
    {
        cout<<head->data<<"\t";;
        head=head->next;
    }
}


int main()
{

    node *LL1=NULL;
    pushback(LL1,10);
    pushback(LL1,20);
    pushback(LL1,50);
    pushback(LL1,90);
    display(LL1);
    
    
return 0;
}
